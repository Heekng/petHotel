package threadInfo;

public class ThreadInfoTest {

}
class Music extends Thread {
	public void musicStart() {
		System.out.println("음악을 재생합니다.");
	}
	@Override
	public void run() {
		while(true) {
			musicStart();
		}
	}
}
class Movie extends Thread {
	public void movieStart() {
		System.out.println("동영상을 재생합니다.");
	}
	@Override
	public void run() {
		while(true) {
			movieStart();
		}
	}
	
}