package 예외;

public class ExceptionEx_02 {
	public static void main(String[] args) {
		method1();
		method2();
		System.out.println("데이터페이스 종료");
	}
	
	private static void a() {
		b();
	}
	private static void b() {
		int a = 10;
		int c = 1/0;
	}
	private static void method1() {

	}
	private static void method2() {
		a();
	}
}
